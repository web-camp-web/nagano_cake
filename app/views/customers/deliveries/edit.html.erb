<div class="container">
  <div colass="row">
    <div>
      <h2>配送先編集</h2>
      <%= form_with model: @delivery, url: delivery_path, method: :patch, local: true do |f| %>
        <div>
          <%= f.label :"郵便番号(ハイフンなし)" %>
          <%= f.text_field :postcode %>
        </div>
        <div>
          <%= f.label :"住所" %>
          <%= f.text_field :address %>
        </div>
        <div>
          <%= f.label :"宛名" %>
          <%= f.text_field :name %>
          <%= f.submit :"変更内容を保存" %>
        </div>
      <% end %>
    </div>
  </div>
</div>